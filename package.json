{
  "name": "qotes-nippon-colors",
  "homepage": "https://qotes.github.io/Colors",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/css --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "collect": "python scripts/q.py",
    "ci": "react-scripts test --ci --coverage --env=jsdom",
    "ci-jest": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.8",
    "@types/react": "16.9.6",
    "@types/react-dom": "16.9.1",
    "gh-pages": "^2.0.0",
    "node-fetch": "^2.2.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "3.2.0",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
